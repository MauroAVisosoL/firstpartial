def suma(a,b):
  x = a + b
  return x

def resta(a,b):
  x = a - b
  return x

def multiplicacion(a,b):
  x = a * b
  return x

def division(a,b):
  x = a / b
  return x

print("Give me your first number:")
a = int(input())
print("Give me your second number:")
b = int(input())
print("The summary is:", suma(a,b), "and the substraction is:", resta(a,b))
print("Multiplication is:", multiplicacion(a,b), "and the division is:", division(a,b))
